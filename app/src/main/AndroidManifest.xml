    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyGroceryStore">
        <activity android:name=".activities.AdminAddNewActivity"></activity>
        <activity android:name=".activities.AdminHomeActivity" />
        <activity android:name=".activities.AdminActivity" />
        <activity android:name=".activities.PlacedOrderActivity" />
        <activity android:name=".activities.NavCategoryActivity" />
        <activity android:name=".activities.DetailedActivity" />
        <activity android:name=".adapter.ViewAllActivity" />
        <activity android:name=".activities.DisplayActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.RegistrationActivity">

            <!-- <intent-filter> -->
            <!-- <action android:name="android.intent.action.MAIN" /> -->


            <!-- <category android:name="android.intent.category.LAUNCHER" /> -->
            <!-- </intent-filter> -->
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>